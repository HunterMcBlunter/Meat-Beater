Module HideObjects

    Public Sub HidePowerTug(prPowerTugTitle, prPowerTugButt, prPowerTugNumberOwnedLbl, prPowerTugBPSLbl, prPowerTugDesc) 'Hides a specified Power Tug

        prPowerTugTitle.Hide()
        prPowerTugButt.Hide()
        prPowerTugNumberOwnedLbl.Hide()
        prPowerTugBPSLbl.Hide()
        prPowerTugDesc.Hide()

    End Sub

End Module
